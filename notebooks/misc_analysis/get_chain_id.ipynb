    "from web3 import Web3\n",
    "\n",
    "# rpc_url = 'https://rpc.river.build'\n",
    "rpc_url = 'https://mainnet.hsk.xyz'\n",
    "\n",
    "# Connect to the RPC endpoint\n",
    "w3 = Web3(Web3.HTTPProvider(rpc_url))\n",
    "\n",
    "# Get the chain ID\n",
    "chain_id = w3.eth.chain_id\n",
    "\n",
    "print(f\"The chain ID is: {chain_id}\")\n"
   ]
